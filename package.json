{
  "name": "shortbread",
  "version": "0.4.1",
  "description": "Asynchronous, non-blocking loading pattern for CSS and JavaScript resources",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && eslint **/*.js && istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "babel lib --presets es2015-script --out-dir build",
    "prepublish": "npm run build",
    "php": "npm run build && node test/php/php.js && php -S 0.0.0.0:8080 -t tmp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkphl/shortbread.git"
  },
  "keywords": [
    "css",
    "stylesheet",
    "javascript",
    "js",
    "cache",
    "cookie",
    "loader",
    "loadcss",
    "loadjs",
    "preload",
    "async",
    "defer"
  ],
  "author": "Joschi Kuphal <https://jkphl.is>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkphl/shortbread/issues"
  },
  "homepage": "https://github.com/jkphl/shortbread#readme",
  "dependencies": {
    "crypto": "1.0.1",
    "fg-loadcss": "^2.0.1",
    "is-url": "^1.2.2",
    "through2": "^2.0.3",
    "uglify-js": "^3.0.18",
    "vinyl": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015-script": "^1.1.0",
    "coveralls": "*",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "fs-extra": "^4.0.3",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.0.0",
    "gulp-template": "^4.0.0",
    "handlebars": "^4.0.10",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "should": "^13.1.3",
    "stream-assert": "^2.0.3",
    "vinyl-file": "^3.0.0"
  },
  "files": [
    "build",
    "doc",
    "lib"
  ],
  "engines": {
    "node": ">=4.0.0"
  }
}
